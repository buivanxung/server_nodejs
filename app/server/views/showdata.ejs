<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title> Data LoRa   </title>
  </head>

  <body>
     <table border="1">
       <tr bgcolor = "#aaffff">
         <td>ID</td>
         <td>MAC device</td>
         <td>Time</td>
         <td>RSSI</td>
         <td>Timestamp</td>
         <td>frequency</td>
         <td>channel</td>
         <td>rfChain</td>
         <td>crcStatus</td>
         <td>codeRate</td>
         <td>loRaSNR</td>
         <td>size</td>
         <td>modulation</td>
         <td>spreadFactor</td>
         <td>bandwidth</td>
         <td>phyPayload</td>
       </tr>

       <% list.rows.forEach(function(d) { %>
         <tr>
           <td><%= d.id %></td>
           <td><%= d.mac %></td>
           <td><%= d.time %></td>
           <td><%= d.rssi %></td>
           <td><%= d.timestamp %></td>
           <td><%= d.frequency %></td>
           <td><%= d.channel %></td>
           <td><%= d.rfchain %></td>
           <td><%= d.crcstatus %></td>
           <td><%= d.coderate %></td>
           <td><%= d.lorasnr %></td>
           <td><%= d.size %></td>
           <td><%= d.modulation %></td>
           <td><%= d.spreadfactor %></td>
           <td><%= d.bandwidth %></td>
           <td><%= d.phypayload %></td>

         </tr>
     <% }); %>
     </table>
  </body>
</html>
